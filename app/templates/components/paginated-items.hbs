<ul class="large-block-grid-5 small-block-grid-2 list">
  {{#each pagedContent key="@index" as |item|}}
    <li>
      <img src="{{item.displayImageUrl}}" />
      <li>{{item.donorDescription}}</li>
    </li>
  {{/each}}
</ul>

{{#if showPaginationBar}}
  {{page-numbers content=pagedContent}}
{{/if}}
